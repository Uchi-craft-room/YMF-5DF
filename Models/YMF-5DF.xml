  <model>
    <name>shadowOSG</name>
    <path>Aircraft/YMF-5DF/Models/shadow.xml</path>
	<offsets>
		<x-m> 0 </x-m>
		<y-m> 0 </y-m>
		<z-m> -1.0 </z-m>
	</offsets>
  </model>

 <animation>
    <type>select</type>
    <object-name>shadowOSG</object-name>
    <condition>
      <and>
      <equals>
        <property>/sim/rendering/rembrandt/enabled</property>
        <value>false</value>
      </equals>
      <equals>
        <property>/sim/replay/replay-state</property>
        <value>0</value>
      </equals>
      <property>gear/gear/ground-is-solid</property>
      </and>
    </condition>
  </animation>

<!-- Pushback -->
<model>
 <name>Pushback</name>
