<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>ai.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>background</object-name>
    <object-name>scale</object-name>
    <object-name>float</object-name>
    <object-name>aircraft</object-name>
    <object-name>bouton</object-name>
    <object-name>slip</object-name>
  </animation>


  <animation>
    <type>material</type>
    <object-name>background</object-name>
    <object-name>scale</object-name>
    <object-name>float</object-name>
    <object-name>aircraft</object-name>
    <object-name>slip</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>background</object-name>
    <object-name>scale</object-name>
    <object-name>float</object-name>
    <property>/orientation/roll-deg</property>
    <factor>1.0</factor>
    <axis>
      <x>1.0</x>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>float</object-name>
    <property>/orientation/pitch-deg</property>
    <factor>-0.00060</factor>
    <min-m>-0.02</min-m>
    <max-m>0.02</max-m>
    <axis>
      <z>1.0</z>
    </axis>
  </animation>

</PropertyList>








