        <control-speed control="FLAP1" transition-time="0.01"/>
  </wing>

  <mstab x="-0.3" y="0.5" z="-0.5" taper="1.0" length="3.5" chord="1.5" camber="-0.005" sweep="0" incidence="0" idrag="1.5"> 
