   </gear>

<!--  FLOATS -->

  <gear x="0.0" y="1.5" z="-2.1"  sfric="0.4" dfric="0.4" on-water="1" on-solid="1" ignore-by-solver="1" reduce-friction-by-extension="1.3" speed-planing="400" spring-factor-not-planing="3">
